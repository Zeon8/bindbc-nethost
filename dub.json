	"description": "D bindings for .NET Runtime hosting API",
	"license": "MIT",
	"name": "bindbc-nethost",
	"dependencies": {
		"bindbc-loader": "~>1.0.0"
	},
	"targetType": "library",
	"configurations": [
		{
			"name": "dynamic",
			"versions": ["BindNethost_Dynamic"]
		},
		{
			"name": "dynamicBC",
			"versions": ["BindNethost_Dynamic"],
			"subConfigurations": {
				"bindbc-loader": "yesBC"
			},
			"dflags": ["-betterC"]
		},
		{
			"name": "static",
			"versions": ["BindNethost_Static"],
			"excludedSourceFiles": ["source/bindbc/nethost/binddynamic.d"]
		},
		{
			"name": "staticBC",
			"versions": ["BindNethost_Static"],
			"excludedSourceFiles": ["source/bindbc/nethost/binddynamic.d"],
			"subConfigurations": {
				"bindbc-loader": "yesBC"
			},
			"dflags": ["-betterC"]
		}
	]
}